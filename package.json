{
  "name": "medis",
  "description": "GUI for Redis",
  "productName": "Medis",
  "version": "1.0.3",
  "electronVersion": "4.0.0",
  "license": "MIT",
  "author": "luin <i@zihua.li> (http://zihua.li)",
  "main": "dist/main",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "webpack",
    "watch": "cross-env WEBPACK_WATCH=true run-s build",
    "start": "electron .",
    "pack": "cross-env NODE_ENV=production run-s build pack-mas",
    "pack-mas": "node bin/pack.js",
    "pack-x": "run-p pack-win32 pack-darwin pack-linux",
    "pack-win32": "electron-packager . --platform=win32 --arch=x64 --out dist",
    "pack-linux": "electron-packager . --platform=linux --arch=x64 --out dist",
    "pack-darwin": "electron-packager . --platform=darwin --arch=x64 --out dist",
    "zip-x": "run-p zip-win32 zip-linux zip-darwin",
    "zip-win32": "cd dist && jszip-cli add --level 9 --quiet --output Medis-win32-x64.zip Medis-win32-x64 ",
    "zip-linux": "cd dist && jszip-cli add --level 9 --quiet --output Medis-linux-x64.zip Medis-linux-x64",
    "zip-darwin": "cd dist && jszip-cli add --level 9 --quiet --output Medis-darwin-x64.zip Medis-darwin-x64",
    "release": "cross-env NODE_ENV=production run-s build pack-x zip-x"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/luin/medis.git"
  },
  "dependencies": {
    "electron": "^4.2.2",
    "electron-context-menu": "^0.12.1",
    "fixed-data-table-contextmenu": "^1.7.2",
    "ioredis": "^4.9.3",
    "jquery": "^3.4.1",
    "jquery.terminal": "^2.5.1",
    "lodash.escape": "^4.0.1",
    "lodash.sortedindexby": "^4.6.0",
    "medis-react-codemirror": "^1.1.1",
    "redis-commands": "^1.5.0",
    "ssh2": "^0.6.2",
    "xterm": "^3.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@ffflorian/jszip-cli": "^3.1.0",
    "@types/jquery": "^3.3.29",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/redux-actions": "^2.6.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "codemirror": "^5.46.0",
    "cross-env": "^7.0.2",
    "css-loader": "^2.1.1",
    "electron-osx-sign": "^0.4.4",
    "electron-packager": "^13.1.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "human-format": "^0.10.1",
    "immutable": "^3.8.1",
    "json-editor": "^0.7.23",
    "jsonlint": "^1.6.2",
    "jsx-loader": "^0.13.2",
    "lint": "^1.1.2",
    "lodash.clone": "^4.5.0",
    "lodash.zip": "^4.2.0",
    "mini-css-extract-plugin": "^0.6.0",
    "minimatch": "^3.0.4",
    "msgpack5": "^4.2.1",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-addons-css-transition-group": "^15.5.2",
    "react-document-title": "^2.0.1",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-sortable-hoc": "^1.9.1",
    "react-split-pane": "^0.1.87",
    "redis-splitargs": "^1.0.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "reselect": "^4.0.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^7.1.0",
    "sortablejs": "^1.9.0",
    "typescript": "^3.4.5",
    "url-loader": "^1.1.2",
    "webpack": "^4.31.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.2"
  }
}
